       <!-- SELECT2 EXAMPLE -->
       <div class="card card-default">
        <div class="card-header">
          <h3 class="card-title">Hasil Wawancara</h3>

          <div class="card-tools">
            <button type="button" class="btn btn-tool" data-card-widget="collapse">
              <i class="fas fa-minus"></i>
            </button>
            <button type="button" class="btn btn-tool" data-card-widget="remove">
              <i class="fas fa-times"></i>
            </button>
          </div>
        </div>
        <!-- /.card-header -->
          <div class="card-body">
            <form action="/admin/postWawancara" id="formUpload" method="post" enctype="multipart/form-data">
                <div class="row">
                    <div class="col-md-12">
                        <label for="exampleInputFile">Keterangan :</label>
                            <input type="hidden" name="id_pelamar" value="<%= id %>" id="">
                            <textarea name="keterangan" id="" rows="3" class="form-control"></textarea>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <label for="exampleInputFile">Pilihan :</label>
                        <div class="custom-control custom-radio">
                            <input class="custom-control-input" name="pilihan[]" value="2" type="radio" id="customRadio1">
                            <label for="customRadio1" class="custom-control-label">Lulus Wawancara</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input class="custom-control-input" name="pilihan[]" value="3" type="radio" id="customRadio2">
                            <label for="customRadio2" class="custom-control-label">Tdk Lulus Wawancara</label>
                          </div>
                    </div>
                </div>
                <button type="submit" class="submitButton btn btn-primary btn-block" style="margin-top:15px;">Submit</button>
            </div>
          </form>
            <!-- /.row -->
          </div>